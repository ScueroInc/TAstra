<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="OPEN_SOURCE" transaction-type="JTA">
	
	<jta-data-source>java:/JPADS</jta-data-source>
	<class>entity.Empleado</class>
	<class>entity.Encargado</class>
	<class>entity.Cronograma</class>
	<class>entity.Pedido</class>
	<class>entity.Maquina</class>
	<class>entity.Revision</class>
	<class>entity.Reporte</class>
	<properties>
	<property name="javax.persistence.schema-generation.database.action" value="none"/>
	</properties>
	</persistence-unit>
</persistence>
